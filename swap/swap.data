#Alliance - S4

begin
variables f, i, t

	#`f`: Hasta donde contar
	i=0
	f=20
	:inicio

	#`i`: Iterador
	i=i+1
	
	#Imprimir el contador
	print i

	#`t`: Comparador entre `i` y `f`
	t=f-i
	#De no ser iguales, salta a inicio

	#esperar
	io HDD1 3
	jnz t inicio
end


