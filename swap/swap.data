#!/usr/bin/ansisop
begin
variables f, i, t

	#`f`: Hasta donde contar
	f=20
	#`i`: Iterador
	i=0
	
	:inicio

	#Iterar
	i=i+1
	
	#Imprimir el contador
	print i

	#`t`: Comparador entre `i` y `f`
	t=f-i
	#De no ser iguales, salta a inicio
	jnz t inicio

end


